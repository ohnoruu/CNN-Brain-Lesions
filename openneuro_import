import requests
import os

image_file_urls = [

]

labels_file_urls = [

]

image_output_dir = 'data\MRI-DWI'
labels_output_dir = 'data\annotations'

# images/unlabeled MRI Data
if not os.path.exists(image_output_dir):
    os.makedirs(image_output_dir)

for url in image_file_urls:
    filename = os.path.join(image_output_dir, os.path.basename(url))
    response = requests.get(url)
    with open(filename, 'wb') as file:
        file.write(response.content)
    print(f"Downloaded {url} to {filename}")

# annotations
if not os.path.exists(labels_output_dir):
    os.makedirs(labels_output_dir)

for url in labels_file_urls:
    filename = os.path.join(labels_output_dir, os.path.basename(url))
    response = requests.get(url)
    with open(filename, 'wb') as file:
        file.write(response.content)
    print(f"Downloaded {url} to {filename}")

print("Upload complete.")